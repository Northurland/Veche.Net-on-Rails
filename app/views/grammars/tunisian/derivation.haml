          %td.arrow →
          %td dolcs [ˈdolks]
          %td sweetness
        %tr
          %td grav [ˈgraːv]
          %td sad
          %td.arrow →
          %td gravs [ˈgraːvz]
          %td sadness
        %tr
          %td forot [ˈfɔ.rot]
          %td strong
          %td.arrow →
          %td forots [ˈfɔ.rots]
