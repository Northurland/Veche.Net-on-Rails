          %td cretator [krə.ˈtaː.tor]
          %td cretatre [krə.ˈta.trə]
          %td creator
        %tr
          %td imâ [i.ˈmaː]
          %td love
          %td.arrow →
          %td imator [ˈi.mʌ.tor]
          %td imatre [ˈi.mʌ.trə]
          %td lover
        %tr
          %td yscribey [ˈis.kri.bej]
          %td write
          %td.arrow →
          %td yscribor [ˈis.kri.bor]
          %td yscribre [ˈis.kri.brə]
