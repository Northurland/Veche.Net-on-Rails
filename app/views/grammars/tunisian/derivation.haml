      %thead
        %tr
          %th Word
          %th Meaning
          %th
          %th Person (M)
          %th Person (F)
          %th Meaning
      %tbody
        %tr
          %td cretá [krə.ˈtaː]
          %td create
          %td.arrow →
          %td cretator [krə.ˈtaː.toːr]
          %td cretatre [krə.ˈtaː.trə]
          %td creator
        %tr
          %td imá [i.ˈmaː]
          %td love
          %td.arrow →
          %td imator [ˈi.mʌ.toːr]
          %td imatre [ˈi.mʌ.trə]
          %td lover
        %tr
          %td yscribey [ˈis.kri.bej]
          %td write
