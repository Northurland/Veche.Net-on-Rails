          %td Tenès [tə.ˈnɛs]
          %td Tunisia
          %td.arrow →
          %td tenèsiá [tə.ˈne.sjaː]
          %td Tunisian
        %tr
          %td Ambricce [ˈaːm.bri.kə]
          %td America
          %td.arrow →
          %td ambriçiâá [ˈaːm.bri.ʃaː]
          %td American
        %tr
          %td Yorupe [ˈjɔ.ru.pə]
          %td Europe
          %td.arrow →
          %td yorupiá [ˈjɔ.ru.pjaː]
          %td European
        %tr
          %td Aigltère [aj.gu.ˈte.rə]
          %td England
          %td.arrow →
          %td aiglis [ˈaj.glis]
          %td English
        %tr
