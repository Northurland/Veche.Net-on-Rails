        %tr
          %td Ambricce [ˈaːm.bri.kə]
          %td America
          %td.arrow →
          %td ambriçiâ [ˈaːm.bri.ʃaː]
          %td American
        %tr
          %td Iorupe [ˈjɔ.ru.pə]
          %td Europe
          %td.arrow →
          %td iorupiâ [ˈjɔ.ru.pjaː]
          %td European
        %tr
          %td Aigltére [aj.gu.ˈte.rə]
