          %td.arrow →
          %td dolcs [ˈdoːlks]
          %td sweetness
        %tr
          %td grav [ˈgraːv]
          %td sad
          %td.arrow →
          %td gravs [ˈgraːvz]
          %td sadness
        %tr
          %td forut [ˈfɔ.rut]
          %td strong
          %td.arrow →
          %td foruts [ˈfɔ.ruts]
