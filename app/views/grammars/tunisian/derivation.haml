              %td live
              %td.arrow →
              %td navivey [ˈnaː.ˈvi.vej]
              %td revive
            %tr
              %td noví [nu.ˈviː]
              %td be new
              %td.arrow →
              %td nanoví [ˈnaː.nu.ˈviː]
