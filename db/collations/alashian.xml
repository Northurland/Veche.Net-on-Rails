<!-- Alashian Collation for MySQL 5.6 -->
<!--
  To install:
  1. Locate Index.xml in the MySQL character sets directory by running
     `SHOW VARIABLES LIKE 'character_sets_dir';`
  2. Verify the collation ID defined by the <collation> tag here is
     not already in use by running
     `SELECT * FROM INFORMATION_SCHEMA.COLLATIONS WHERE ID={ID}`
  3. Add the following XML into Index.xml inside the
     <charset name="utf8">...</charset> block.
  4. Save, exit, and restart MySQL.
-->
<collation name="utf8_alashian_ci" id="1201">
  <rules>
    <!-- Sort Β̄ as a separate letter after Β -->
    <reset>β</reset>
    <p>в̄</p>

    <!-- Define case variants of Β̄ -->
    <reset>в̄</reset>
    <t>Β̄</t>

    <!-- Sort Γ after Β̄ -->
    <reset>в̄</reset>
    <p>γ</p>

    <!-- Sort Γ̄ as a separate letter after Γ -->
    <reset>γ</reset>
    <p>γ̄</p>

    <!-- Define case variants of Γ̄ -->
    <reset>γ̄</reset>
    <t>Γ̄</t>

    <!-- Sort Δ after Γ̄ -->
    <reset>γ̄</reset>
    <p>δ</p>

    <!-- Sort Δ̄ as a separate letter after Δ -->
    <reset>δ</reset>
    <p>δ̄</p>

    <!-- Define case variants of Δ̄ -->
    <reset>δ̄</reset>
    <t>Δ̄</t>

    <!-- Sort Ε after Δ̄ -->
    <reset>δ̄</reset>
    <p>ε</p>

    <!-- Sort ΕΙ as a separate letter after Ε -->
    <reset>ε</reset>
    <p>ει</p>

    <!-- Define case variants of ΕΙ -->
    <reset>ει</reset>
    <t>ΕΙ</t>
    <t>Ει</t>

    <!-- Sort Ζ after ΕΙ -->
    <reset>ει</reset>
    <p>ζ</p>

    <!-- Define ΟΥ as a distinct letter to be sorted after Ν -->
    <reset>ν</reset>
    <p>ου</p>

    <!-- Define case variants of ΟΥ -->
    <reset>ου</reset>
    <t>ΟΥ</t>
    <t>Ου</t>

    <!-- Sort Π after ΟΥ -->
    <reset>ου</reset>
    <p>π</p>

    <!-- Sort Ρ̄ as a separate letter after Ρ -->
    <reset>ρ</reset>
    <p>ρ̄</p>

    <!-- Define case variants of Ρ̄ -->
    <reset>ρ̄</reset>
    <t>Ρ̄</t>

    <!-- Sort Σ after Ρ̄ -->
    <reset>ρ̄</reset>
    <p>σ</p>

    <!-- Sort Σ̄ as a separate letter after Σ -->
    <reset>σ</reset>
    <p>σ̄</p>

    <!-- Define case variants of Σ̄ -->
    <reset>σ̄</reset>
    <t>Σ̄</t>
    <t>ς̄</t>

    <!-- Sort Τ after Σ̄ -->
    <reset>σ̄</reset>
    <p>τ</p>

    <!-- Sort ΤΖ as a separate letter after Τ -->
    <reset>τ</reset>
    <p>τζ</p>

    <!-- Define lowercase variants of ΤΖ -->
    <reset>τζ</reset>
    <t>ΤΖ</t>
    <t>Τζ</t>

    <!-- Sort Υ after ΤΖ -->
    <reset>τζ</reset>
    <p>υ</p>

    <!-- Base case variants and ordering -->
    <reset>α</reset><t>Α</t><p>β</p>
    <reset>β</reset><t>Β</t>
    <reset>γ</reset><t>Γ</t>
    <reset>δ</reset><t>Δ</t>
    <reset>ε</reset><t>Ε</t>
    <reset>ζ</reset><t>Ζ</t><p>η</p>
    <reset>η</reset><t>Η</t><p>θ</p>
    <reset>θ</reset><t>Θ</t><p>ι</p>
    <reset>ι</reset><t>Ι</t><p>κ</p>
    <reset>κ</reset><t>Κ</t><p>λ</p>
    <reset>λ</reset><t>Λ</t><p>μ</p>
    <reset>μ</reset><t>Μ</t><p>ν</p>
    <reset>ν</reset><t>Ν</t><p>ο</p>
    <reset>ο</reset><t>Ο</t>
    <reset>π</reset><t>Π</t><p>ρ</p>
    <reset>ρ</reset><t>Ρ</t>
    <reset>σ</reset><t>Σ</t><t>ς</t>
    <reset>τ</reset><t>Τ</t>
    <reset>υ</reset><t>Υ</t><p>φ</p>
    <reset>φ</reset><t>Φ</t><p>χ</p>
    <reset>χ</reset><t>Χ</t><p>ω</p>
    <reset>ω</reset><t>Ω</t>
  </rules>
</collation>
