<!-- Novegradian Collation for MySQL 5.6 -->
<!--
  To install:
  1. Locate Index.xml in the MySQL character sets directory by running
     `SHOW VARIABLES LIKE 'character_sets_dir';`
  2. Verify the collation ID defined by the <collation> tag here is
     not already in use by running
     `SELECT * FROM INFORMATION_SCHEMA.COLLATIONS WHERE ID={ID}`
  3. Add the following XML into Index.xml inside the
     <charset name="utf8">...</charset> block.
  4. Save, exit, and restart MySQL.
-->
<collation name="utf8_novegradian_ci" id="1200">
  <rules>
    <!-- Sort ГЬ as a separate letter after Г -->
    <reset>г</reset>
    <p>гь</p>

    <!-- Define case variants of ГЬ -->
    <reset>гь</reset>
    <t>ГЬ</t>
    <t>Гь</t>

    <!-- Sort Ғ after ГЬ -->
    <reset>гь</reset>
    <p>ғ</p>

    <!-- Define case variants of Ғ -->
    <reset>ғ</reset>
    <t>Ғ</t>

    <!-- Sort ҒЬ as a separate letter after Ғ -->
    <reset>ғ</reset>
    <p>ғь</p>

    <!-- Define case variants of ҒЬ -->
    <reset>ғь</reset>
    <t>ҒЬ</t>
    <t>Ғь</t>

    <!-- Sort Д after ҒЬ -->
    <reset>ғь</reset>
    <p>д</p>

    <!-- Define Ѐ as an accented variant of Е -->
    <reset>е</reset>
    <s>ѐ</s>

    <!-- Define case variants of Ѐ -->
    <reset>ѐ</reset>
    <t>Ѐ</t>

    <!-- Sort Ё as a separate letter after Е -->
    <reset>е</reset>
    <p>ё</p>

    <!-- Sort Ѕ after З -->
    <reset>з</reset>
    <p>ѕ</p>

    <!-- Define case variants of Ѕ -->
    <reset>ѕ</reset>
    <t>Ѕ</t>

    <!-- Sort КЬ as a separate letter after К -->
    <reset>к</reset>
    <p>кь</p>

    <!-- Define case variants of КЬ -->
    <reset>кь</reset>
    <t>КЬ</t>
    <t>Кь</t>

    <!-- Sort Л after КЬ -->
    <reset>кь</reset>
    <p>л</p>

    <!-- Sort Љ after Л -->
    <reset>л</reset>
    <p>љ</p>

    <!-- Define case variants of Љ -->
    <reset>љ</reset>
    <t>Љ</t>

    <!-- Sort М after Љ -->
    <reset>љ</reset>
    <p>м</p>

    <!-- Sort НЬ as a separate letter after Н -->
    <reset>н</reset>
    <p>нь</p>

    <!-- Define case variants of НЬ -->
    <reset>нь</reset>
    <t>НЬ</t>
    <t>Нь</t>

    <!-- Sort О after НЬ -->
    <reset>нь</reset>
    <p>о</p>

    <!-- Define Ў as an accented variant of У -->
    <reset>у</reset>
    <s>ў</s>

    <!-- Define case variants of Ў -->
    <reset>ў</reset>
    <t>Ў</t>

    <!-- Sort ХЬ as a separate letter after Х -->
    <reset>х</reset>
    <p>хь</p>

    <!-- Define case variants of ХЬ -->
    <reset>хь</reset>
    <t>ХЬ</t>
    <t>Хь</t>

    <!-- Sort Ц after ХЬ -->
    <reset>хь</reset>
    <p>ц</p>

    <!-- Sort Ѣ after Ш -->
    <reset>ш</reset>
    <p>ѣ</p>

    <!-- Define case variants of Ѣ -->
    <reset>ѣ</reset>
    <t>Ѣ</t>

    <!-- Sort Ю after Ѣ -->
    <reset>ѣ</reset>
    <p>ю</p>

    <!-- Sort Ч after Я -->
    <reset>я</reset>
    <p>ч</p>

    <!-- Sort Ы after Ч -->
    <reset>ч</reset>
    <p>ы</p>

    <!-- Base case variants and ordering -->
    <reset>а</reset><t>А</t><p>б</p>
    <reset>б</reset><t>Б</t><p>в</p>
    <reset>в</reset><t>В</t><p>г</p>
    <reset>г</reset><t>Г</t>
    <reset>д</reset><t>Д</t><p>е</p>
    <reset>е</reset><t>Е</t>
    <reset>ё</reset><t>Ё</t><p>ж</p>
    <reset>ж</reset><t>Ж</t><p>з</p>
    <reset>з</reset><t>З</t><p>и</p>
    <reset>и</reset><t>И</t><p>й</p>
    <reset>й</reset><t>Й</t><p>к</p>
    <reset>к</reset><t>К</t>
    <reset>л</reset><t>Л</t>
    <reset>м</reset><t>М</t><p>н</p>
    <reset>н</reset><t>Н</t>
    <reset>о</reset><t>О</t><p>п</p>
    <reset>п</reset><t>П</t><p>р</p>
    <reset>р</reset><t>Р</t><p>с</p>
    <reset>с</reset><t>С</t><p>т</p>
    <reset>т</reset><t>Т</t><p>у</p>
    <reset>у</reset><t>У</t><p>х</p>
    <reset>х</reset><t>Х</t>
    <reset>ц</reset><t>Ц</t>
    <reset>ч</reset><t>Ч</t><p>ш</p>
    <reset>ш</reset><t>Ш</t>
    <reset>ы</reset><t>Ы</t>
    <reset>ю</reset><t>Ю</t><p>я</p>
    <reset>я</reset><t>я</t>
  </rules>
</collation>
