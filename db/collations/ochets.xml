<!-- Ochets Collation for MySQL 5.6 -->
<!--
  To install:
  1. Locate Index.xml in the MySQL character sets directory by running
     `SHOW VARIABLES LIKE 'character_sets_dir';`
  2. Verify the collation ID defined by the <collation> tag here is
     not already in use by running
     `SELECT * FROM INFORMATION_SCHEMA.COLLATIONS WHERE ID={ID}`
  3. Add the following XML into Index.xml inside the
     <charset name="utf8">...</charset> block.
  4. Save, exit, and restart MySQL.
-->
<collation name="utf8_ochets_ci" id="1202">
  <rules>
    <!-- Sort Ә after А -->
    <reset>а</reset>
    <p>ә</p>

    <!-- Define case variants of Ә -->
    <reset>ә</reset>
    <t>Ә</t>

    <!-- Sort Б after Ә -->
    <reset>ә</reset>
    <p>б</p>

    <!-- Sort Ҕ after Г -->
    <reset>г</reset>
    <p>ҕ</p>

    <!-- Define case variants of Ҕ -->
    <reset>ҕ</reset>
    <t>Ҕ</t>

    <!-- Sort Д after Ҕ -->
    <reset>ҕ</reset>
    <p>д</p>

    <!-- Sort Ѕ after З -->
    <reset>з</reset>
    <p>ѕ</p>

    <!-- Define case variants of Ѕ -->
    <reset>ѕ</reset>
    <t>Ѕ</t>

    <!-- Sort И after Ѕ -->
    <reset>ѕ</reset>
    <p>и</p>

    <!-- Sort Қ after К -->
    <reset>к</reset>
    <p>қ</p>

    <!-- Define case variants of Қ -->
    <reset>қ</reset>
    <t>Қ</t>

    <!-- Sort Л after Қ -->
    <reset>қ</reset>
    <p>л</p>

    <!-- Sort Ҥ after Н -->
    <reset>н</reset>
    <p>ҥ</p>

    <!-- Define case variants of Ҥ -->
    <reset>ҥ</reset>
    <t>Ҥ</t>

    <!-- Sort О after Ҥ -->
    <reset>ҥ</reset>
    <p>о</p>

    <!-- Sort Ө after О -->
    <reset>о</reset>
    <p>ө</p>

    <!-- Define case variants of Ө -->
    <reset>ө</reset>
    <t>Ө</t>

    <!-- Sort П after Ө -->
    <reset>ө</reset>
    <p>п</p>

    <!-- Sort Һ after С -->
    <reset>с</reset>
    <p>һ</p>

    <!-- Define case variants of Һ -->
    <reset>һ</reset>
    <t>Һ</t>

    <!-- Sort Т after Һ -->
    <reset>һ</reset>
    <p>т</p>

    <!-- Sort Ү after У -->
    <reset>у</reset>
    <p>ү</p>

    <!-- Define case variants of Ү -->
    <reset>ү</reset>
    <t>Ү</t>

    <!-- Sort Ф after Ү -->
    <reset>ү</reset>
    <p>ф</p>

    <!-- Sort Џ after Ч -->
    <reset>ч</reset>
    <p>џ</p>

    <!-- Define lowercase variants of Џ -->
    <reset>џ</reset>
    <t>Џ</t>

    <!-- Sort Ш after Џ -->
    <reset>џ</reset>
    <p>ш</p>

    <!-- Base case variants and ordering -->
    <reset>а</reset><t>А</t>
    <reset>б</reset><t>Б</t><p>в</p>
    <reset>в</reset><t>В</t><p>г</p>
    <reset>г</reset><t>Г</t>
    <reset>д</reset><t>Д</t><p>е</p>
    <reset>е</reset><t>Е</t><p>ж</p>
    <reset>ж</reset><t>Ж</t><p>з</p>
    <reset>з</reset><t>З</t>
    <reset>и</reset><t>И</t><p>к</p>
    <reset>к</reset><t>К</t>
    <reset>л</reset><t>Л</t><p>м</p>
    <reset>м</reset><t>М</t><p>н</p>
    <reset>н</reset><t>Н</t>
    <reset>о</reset><t>О</t>
    <reset>п</reset><t>П</t><p>р</p>
    <reset>р</reset><t>Р</t><p>с</p>
    <reset>с</reset><t>С</t>
    <reset>т</reset><t>Т</t><p>у</p>
    <reset>у</reset><t>У</t>
    <reset>ф</reset><t>Ф</t><p>х</p>
    <reset>х</reset><t>Х</t><p>ц</p>
    <reset>ц</reset><t>Ц</t><p>ч</p>
    <reset>ч</reset><t>Ч</t>
    <reset>ш</reset><t>Ш</t><p>ы</p>
    <reset>ы</reset><t>Ы</t>
  </rules>
</collation>
